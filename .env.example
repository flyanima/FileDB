# Example environment variables for FinSight AI
# Copy this file to .env.local and fill in the real values.

# Supabase configuration (public keys for frontend)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-public-key

# Supabase service role (backend only, never commit)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Other API keys (example)
OPENAI_API_KEY=your-openai-key
GEMINI_API_KEY=your-gemini-key

# Application settings
NEXT_PUBLIC_APP_NAME=FinSight AI
NEXT_PUBLIC_APP_ENV=development
